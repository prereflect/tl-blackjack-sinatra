<h4>
Dealer's cards                                                   <span>&cir;</span>
  Total points: <%= calculate_total(session[:dealer_hand]) %>
</h4>
<% session[:dealer_hand].each do |card| %>
  <%= generate_pic_url(card) %>
<% end %>

<h4>
  <%= session[:player_name] %>'s cards                           <span>&cir;</span>
  Bet: $<%= session[:player_bet] %>                              <span>&cir;</span>
  Total points: <%= calculate_total(session[:player_hand]) %>
</h4>
<% session[:player_hand].each do |card| %>
  <%= generate_pic_url(card) %>
<% end %>
